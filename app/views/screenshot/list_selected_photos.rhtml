<table><tr>
<% @screenshots.each_with_index do |s,i| %>


<td align='center' valign='top'>
<%= i + 1%><br>
<%= link_to image_tag(s.photo.url(:thumb),:class => 'photo'), s.photo.url(:original), :target => '_blank' %>
</td>

<% if (i+1) % 10 == 0 %>
	</tr><tr>
<% end %>

<% end %>
</tr></table>